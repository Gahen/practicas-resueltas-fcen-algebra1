\begin_layout Standard
\begin_inset Formula 
\[
\#C'=\#\left(\left\{ 1\right\} x\left\{ 1\right\} ...\left\{ 0\right\} ...\left\{ 1\right\} x\left\{ 1\right\} \right)
\]

\end_inset


\end_layout

